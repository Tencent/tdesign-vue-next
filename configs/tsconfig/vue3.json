{
  "extends": "./base.json",
  "compilerOptions": {
    "paths": {
      "@adapter/vue": [
        "packages/adapter/vue/vue3"
      ],
      "@adapter/vue/*": [
        "packages/adapter/vue/vue3/*"
      ],
      "@adapter/hooks": [
        "packages/adapter/hooks/vue3"
      ],
      "@adapter/hooks/*": [
        "packages/adapter/hooks/vue3/*"
      ],
      "@adapter/utils": [
        "packages/adapter/utils/vue3"
      ],
      "@adapter/utils/*": [
        "packages/adapter/utils/vue3/*"
      ],
      "@td/intel": [
        "packages/intel/vue3/src"
      ],
      "@td/intel/*": [
        "packages/intel/vue3/src/*"
      ],
      "@td/components": [
        "packages/components/vue3"
      ]
    }
  }
}
