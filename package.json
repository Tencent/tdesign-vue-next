{
  "name": "tdesign-vue-next",
  "purename": "tdesign",
  "version": "0.5.0",
  "title": "tdesign-vue-next",
  "description": "TDesign Component for vue-next",
  "keywords": [
    "vue",
    "vue3",
    "vue-next",
    "tdesign",
    "typescript"
  ],
  "main": "lib/index-lib.js",
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "unpkg": "dist/tdesign.min.js",
  "jsdelivr": "dist/tdesign.min.js",
  "files": [
    "esm",
    "es",
    "es/index.d.ts",
    "cjs",
    "lib",
    "dist",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "sideEffects": [
    "*.vue",
    "dist/*",
    "site/*",
    "examples/*",
    "es/**/style/**",
    "esm/**/style/**"
  ],
  "scripts": {
    "start": "cd site && npm run dev",
    "site": "cd site && npm run build --debug",
    "site:preview": "cd site && npm run build:preview",
    "prebuild": "rimraf es/* esm/* lib/* dist/*",
    "build": "cross-env NODE_ENV=production rollup -c script/rollup.config.js && npm run build:tsc",
    "build:tsc": "concurrently \"npm:build:tsc-*\"",
    "build:tsc-es": "tsc --emitDeclarationOnly -d --outDir es/",
    "build:tsc-esm": "tsc --emitDeclarationOnly -d --outDir esm/",
    "build:tsc-lib": " tsc --emitDeclarationOnly -d --outDir lib/",
    "lint:fix": "eslint --ext .vue,.js,.ts,.tsx ./src --max-warnings 0 --fix",
    "lint": "eslint --ext .vue,.js,.ts,.tsx ./src --max-warnings 0",
    "test": "npm run test:unit && npm run test:node",
    "test:update": "npm run test:unit-update && npm run test:node-update",
    "test:unit": "jest --config script/test/jest.unit.conf.js",
    "test:unit-update": "jest --config script/test/jest.unit.conf.js --updateSnapshot",
    "test:node": "jest --config script/test/jest.ssr.conf.js",
    "test:node-update": "jest --config script/test/jest.ssr.conf.js --updateSnapshot",
    "test:watch": "jest --config script/test/jest.unit.conf.js --watch",
    "test:coverage": "jest --config script/test/jest.unit.conf.js --coverage",
    "test:e2e": "cypress run --config-file script/test/cypress.json",
    "test:e2e-gui": "cypress open --config-file script/test/cypress.json",
    "changelog": "publish-cli changelog",
    "robot": "publish-cli robot-msg",
    "prepare": "husky install"
  },
  "author": "tdesign",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.14.8",
    "@popperjs/core": "^2.9.3",
    "@types/lodash": "^4.14.175",
    "@types/validator": "^13.6.3",
    "dayjs": "^1.9.7",
    "less": "^4.1.2",
    "lodash": "^4.17.15",
    "tdesign-icons-vue-next": "0.0.2",
    "validator": "^13.5.1"
  },
  "peerDependencies": {
    "vue": ">=3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.15.0",
    "@babel/plugin-transform-modules-commonjs": "^7.9.6",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.5.5",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-url": "^6.0.0",
    "@types/raf": "^3.4.0",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/server-renderer": "^3.2.4",
    "@vue/test-utils": "^2.0.0-rc.12",
    "autoprefixer": "^10.4.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.0",
    "camelcase": "^6.2.1",
    "cli-color": "^2.0.1",
    "clipboard": "^2.0.6",
    "commitizen": "^4.0.3",
    "concurrently": "^6.4.0",
    "cross-env": "^7.0.2",
    "css-element-queries": "^1.2.3",
    "cypress": "^9.1.1",
    "cz-conventional-changelog": "^3.0.2",
    "esbuild": "^0.12.19",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^7.17.0",
    "glob": "^7.2.0",
    "husky": "^7.0.4",
    "ignore": "^5.1.9",
    "jest": "^26.0.0",
    "jest-html-reporter": "^3.1.3",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "jest-watch-typeahead": "^0.6.0",
    "lint-staged": "^12.1.2",
    "mockdate": "^3.0.5",
    "postcss": "^8.3.8",
    "prettier": "^2.4.1",
    "prismjs": "^1.17.1",
    "raf": "^3.4.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.55.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-esbuild": "^4.5.0",
    "rollup-plugin-ignore-import": "^1.3.2",
    "rollup-plugin-multi-input": "^1.1.1",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-static-import": "^0.1.1",
    "rollup-plugin-styles": "^3.14.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "tdesign-publish-cli": "^0.0.8",
    "ts-jest": "^26.0.0",
    "typescript": "^4.5.2",
    "vue-jest": "^5.0.0-alpha.7",
    "vue-tsc": "^0.2.3",
    "workbox-precaching": "^6.4.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{vue,js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "git add ."
    ]
  },
  "workspaces": [
    "./site"
  ]
}
